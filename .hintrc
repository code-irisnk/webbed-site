{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  },
  "connector": {
    "name": "local",
    "options": {
      "include": [
        "**/*.html"
      ],
      "exclude": [
        "tumblr-bak/**"
      ]
    }
  }
}
